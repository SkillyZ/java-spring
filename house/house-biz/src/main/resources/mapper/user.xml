<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skilly.house.biz.mapper.UserMapper">

    <select id="selectUsers" resultType="user">
        select name,phone,email from user
    </select>


    <insert id="insert">
        insert into user (
        name,
        phone,
        email,
        aboutme,
        passwd,
        avatar,
        type,
        create_time,
        enable
        )
        values(
        #{name},
        #{phone},
        #{email},
        #{aboutme},
        #{passwd},
        #{avatar},
        #{type},
        #{createTime},
        #{enable}
        )

    </insert>

</mapper>